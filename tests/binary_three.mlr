{- automaton checking if a binary number is divisible by 3 -}

NFA1 :=
            |   _   `0  `1
---------------------------
-> s1 ->    |   !   s1  s2
   s2       |   !   s3  s1
   s3       |   !   s2  s3

REG1 := < NFA1 >

:assert "11" < REG1
:assert "110" < REG1
:assert "1001" < REG1
:assert NOT "1010" < REG1

